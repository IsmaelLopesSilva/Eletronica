*
*
*
* 4 bit binary full adder
* An->Sn Tpd         425n  td1
* An->Cn Tpd         250n  td2 or (td3=td2-td4)
* Ci->Co Tpd         130n  td4
* Ci->Sn Tpd         320n  only modelled with Tpd=td1
* tr                 100n
.SUBCKT CD4008B  A0 A1 A2 A3 B0 B1 B2 B3 CI S0 S1 S2 S3 CO  VDD VGND  vdd1={vdd} speed=1 tripdtb=5n
.param td1=1e-9*(400-40-10)*5.0/(vdd1}*{speed}
.param td2=1e-9*(250-40-10)*5.0/(vdd1}*{speed}
.param td4=1e-9*(130-40-10)*5.0/(vdd1}*{speed}
.param td3=td2-td4
*
XIN1  A0 A0i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb} 
XIN2  A1 A1i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb} 
XIN3  A2 A2i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb} 
XIN4  A3 A3i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb} 
XIN5  B0 B0i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb}
XIN6  B1 B1i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb}
XIN7  B2 B2i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb}
XIN8  B3 B3i  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb}
XIN9  CI CIi  VCC VGND  CD40_IN_1  vdd2={vdd1}  speed2={speed}  tripdt2={tripdtb}
*
XLOGIC  A0i A1i A2i A3i B0i B1i B2i B3i CIi S0i S1i S2i S3i COi  CD4008_i  tripdt2={tripdtb} td1a={td1} td2a={td2} td3a={td3} td4a={td4}
*
XOUT1  S0i S0  VCC VGND  CD40_OUT_1X  vdd2={vdd1} speed2={speed}  tripdt2={tripdtb}
XOUT2  S1i S1  VCC VGND  CD40_OUT_1X  vdd2={vdd1} speed2={speed}  tripdt2={tripdtb}
XOUT3  S2i S2  VCC VGND  CD40_OUT_1X  vdd2={vdd1} speed2={speed}  tripdt2={tripdtb}
XOUT4  S3i S3  VCC VGND  CD40_OUT_1X  vdd2={vdd1} speed2={speed}  tripdt2={tripdtb}
XOUT5  COi CO  VCC VGND  CD40_OUT_1X  vdd2={vdd1} speed2={speed}  tripdt2={tripdtb}
.ends
*
*
.SUBCKT  CD4008_i A0 A1 A2 A3 B0 B1 B2 B3 CI S0 S1 S2 S3 CO  tripdt={tripdt2} td1b={td1a} td2b={td2a} td3b={td3a} td4b={td4a}
A1CIN   ci 0  0 0 0  ci1n   0    0  BUF  tripdt={tripdt}
A1OR0   a0 b0 0 0 0  or10n  or10 0  OR  tripdt={tripdt}
A1AND0  a0 b0 0 0 0  and10n 0    0  AND  tripdt={tripdt}
A1OR1   a1 b1 0 0 0  or11n  or11 0  OR  tripdt={tripdt}
A1AND1  a1 b1 0 0 0  and11n 0    0  AND  tripdt={tripdt}
A1OR2   a2 b2 0 0 0  or12n  or12 0  OR  tripdt={tripdt}
A1AND2  a2 b2 0 0 0  and12n 0    0  AND  tripdt={tripdt}
A1OR3   a3 b3 0 0 0  or13n  or13 0  OR  tripdt={tripdt}
A1AND3  a3 b3 0 0 0  and13n 0    0  AND  tripdt={tripdt}
*
A2INV0  ci1n 0      0 0 0  ci2n  0 0  BUF  tripdt={tripdt}
A2AND0  or10 and10n 0 0 0  0 and20 0  AND  tripdt={tripdt}
A4XOR0  ci2n and20  0 0 0  0    s0 0  XOR  tripdt={tripdt}  td={td1b}
*
A2AND1A  ci1n   and10n 0 0 0  0 and21a 0  AND  tripdt={tripdt}
A2AND1B  or11   and11n 0 0 0  0 and21b 0  AND  tripdt={tripdt}
A3OR1    and21a or10n  0 0 0  or31n  0 0  OR  tripdt={tripdt}
A4XOR1   or31n  and21b 0 0 0  0     s1 0  XOR  tripdt={tripdt}  td={td1b}
*
A2AND2A  ci1n   and10n and11n 0 0  0 and22a 0  AND  tripdt={tripdt}
A2AND2B  and11n or10n  0      0 0  0 and22b 0  AND  tripdt={tripdt}
A2AND2C  or12   and12n 0      0 0  0 and22c 0  AND  tripdt={tripdt}
A3OR2    and22a and22b or11n  0 0  or32n  0 0  OR  tripdt={tripdt}
A4XOR2   or32n  and22c 0      0 0  0     s2 0  XOR  tripdt={tripdt}  td={td1b}
*
A2AND3A  ci1n   and10n and11n and12n 0  0 and23a 0  AND  tripdt={tripdt}
A2AND3B  and11n and12n or10n  0      0  0 and23b 0  AND  tripdt={tripdt}
A2AND3C  and12n or11n  0      0      0  0 and23c 0  AND  tripdt={tripdt}
A2AND3D  or13   and13n 0      0      0  0 and23d 0  AND  tripdt={tripdt}
A3OR3    and23a and23b and23c or12n  0  or33n  0 0  OR  tripdt={tripdt}
A4XOR3   or33n  and23d 0      0      0  0     s3 0  XOR  tripdt={tripdt}  td={td1b}
*
*A2AND4A  ci1n  and10n and11n and12n and13n  0 and24a 0  AND  tripdt={tripdt}
A2AND4A  0  and10n and11n and12n and13n  0 and24aa 0  AND  tripdt={tripdt} td={td3b}
A2AND4AA ci1n and24aa  0     0      0    0 and24a  0  AND  tripdt={tripdt} td={td4b} 
  
A2AND4B  and11n and12n and13n or10n 0       0 and24b 0  AND  tripdt={tripdt} td={td2b}
A2AND4C  and12n and13n or11n 0      0       0 and24c 0  AND  tripdt={tripdt} td={td2b}
A2AND4D  and13n or12n  0     0      0       0 and24d 0  AND  tripdt={tripdt} td={td2b}
 
A3OR4    and24a and24b and24c and24d or13n  co  0    0  OR  tripdt={tripdt}  
.ends
